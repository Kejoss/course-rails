        <h2 class="font-semibold text-2xl">Questions (<%= @evaluation.questions.count %>) </h2>
    </div>

    <div class="h-screen overflow-auto">
        <%= @evaluation.questions.each do |question| %>
            <!--question es la variable y se la pasa a la pagina question-->
            <%= render 'questions/question', question: question %>
        <% end %>
    </div>

    <div>
        a
    </div>
</div>